{
  "name": "frontend-starter-pack",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@lingui/macro": "^2.9.1",
    "@lingui/react": "^2.9.1",
    "antd": "^3.26.9",
    "axios": "^0.19.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^4.9.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.0.1",
    "tailwindcss": "^1.2.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "prestart": "npm run build:css && npm run i18n:compile",
    "prebuild": "npm run build:css:prod && npm run i18n:compile",
    "build:css": "postcss tailwind.css -o src/tailwind-generated.css",
    "build:css:prod": "postcss tailwind.css -o src/tailwind-generated.css --env production",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "react-app-rewired test",
    "i18n:add": "lingui add-locale",
    "i18n:extract": "lingui extract",
    "i18n:compile": "lingui compile",
    "eject": "react-app-rewired eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "@lingui/cli": "^2.9.1",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.1.0",
    "@types/jest": "^25.1.2",
    "@types/lingui__core": "^2.7.0",
    "@types/lingui__macro": "^2.7.3",
    "@types/lingui__react": "^2.8.1",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-mock-store": "^1.0.2",
    "@types/styled-components": "^4.4.2",
    "autoprefixer": "^9.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^0.9.1",
    "cypress": "^4.0.1",
    "husky": "^4.2.1",
    "less": "^3.11.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.0.7",
    "postcss-cli": "^7.1.0",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "3.3.1",
    "redux-mock-store": "^1.5.4",
    "source-map-explorer": "^2.2.2",
    "typescript": "^3.7.5"
  }
}
